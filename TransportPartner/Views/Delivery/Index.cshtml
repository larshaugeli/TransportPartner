@model IEnumerable<Delivery>

@{
    ViewData["Title"] = "Leveringer";
}

<h2>Courses</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DeliveryId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DeliveryDateTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Address)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CarUsed)
        </th>
        <th></th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DeliveryId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DeliveryDateTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Address)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CarUsed.RegNr)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.DeliveryId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.DeliveryId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.DeliveryId">Delete</a>
            </td>
        </tr>
    }
    </tbody>
</table>